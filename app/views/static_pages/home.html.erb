<body>
  <% if logged_in? %>
  <div class="col-md-8">
      <h3>Atendimentos</h3>
      <%= render 'shared/feed' %>
    </div>
  <div class="row">
    <aside class="col-md-4">
      <section class="paciente_info">
        <%= render 'shared/paciente_info' %>
      </section>
      <section class="atendimento_form">
        <%= render 'shared/atendimento_form' %>
      </section>
    </aside>
    <div class="col-md-4">
      <a href='/hospitals/index'>Ver hospitais cadastrados</a>
    </div>
  </div>
<% else %>
  <h1>SUSTENTAÇÃO</h1>
  <p>Find me in app/views/static_pages/home.html.erb</p>
  <a href='/pacientes/new'> Cadastre-se agora! </a>
  <% end %>
</body>